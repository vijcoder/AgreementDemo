services:
  db:
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: 2Secure*Password2
    image: mcr.microsoft.com/mssql/server:2019-latest
    ports:
    - published: 1433
      target: 1433
  usermanagement:
    build:
      context: C:\Projects\RnD\AgreementDemo
      dockerfile: UserManagement/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: UserManagement
      target: base
    container_name: UserManagement
    depends_on:
      db:
        condition: service_started
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: usermanagement:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/netcoreapp3.1/UserManagement.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    - target: 443
    tty: true
    volumes:
    - C:\Projects\RnD\AgreementDemo\UserManagement:/app:rw
    - C:\Users\VDARJI\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\VDARJI\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\VDARJI\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\VDARJI\.nuget\packages:/root/.nuget/packages:ro
    - C:\Projects\RnD\AgreementDemo:/src:rw
version: '3.4'